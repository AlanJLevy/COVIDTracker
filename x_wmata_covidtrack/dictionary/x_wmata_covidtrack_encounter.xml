        <element attributes="edge_encryption_enabled=true" label="Date Symptoms Started" max_length="40" name="date_symptoms_started" type="glide_date"/>
        <element label="Employee" mandatory="true" max_length="32" name="employee" reference="sys_user" type="reference"/>
