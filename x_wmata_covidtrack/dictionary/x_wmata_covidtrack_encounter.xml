        </element>
        <element label="Last Day Worked On Site" max_length="40" name="last_day_worked_on_site" type="glide_date"/>
        <element label="Last Day Work Information" max_length="10000" name="last_day_work_information" type="string"/>
        <element attributes="encode_utf8=false" label="OHAW Triage Staff Member" max_length="32" name="ohaw_triage_staff_member" reference="sys_user" type="reference"/>
        <element label="COVID-19 Diagnosis" max_length="40" name="potential_covid_19_case" type="boolean"/>
        <element attributes="edge_encryption_enabled=true" label="Recovery Location" max_length="40" name="recovery_location" type="string"/>
        <element attributes="edge_encryption_enabled=true" label="Reporting Agency" max_length="100" name="reporting_agency" type="string"/>
        <element attributes="edge_encryption_enabled=true" label="Symptoms" max_length="10000" name="symptoms" type="string"/>
        <element attributes="edge_encryption_enabled=true" label="Symptom Severity" max_length="40" name="symptom_severity" type="string"/>
