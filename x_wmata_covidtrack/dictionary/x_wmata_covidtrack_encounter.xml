        <element label="Eligible to Work" max_length="40" name="work_eligibility" type="boolean"/>
        <element attributes="edge_encryption_enabled=true" label="Work Status Update Date" max_length="40" name="work_status_update_date" type="glide_date"/>
    </element>
</database>
